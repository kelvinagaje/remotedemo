  <rules name="HashingRule" editor="hashFunctionWizard">
    <properties fieldRulePropertyType="EXPRESSION" value="hash_sha1(${FIELDNAME})"/>
    <properties fieldRulePropertyType="NAME" value="HASH_${FIELDNAME}"/>
  </rules>
